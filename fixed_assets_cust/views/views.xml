<odoo>
  <data>
      <record id="view_account_asset_form_inherits" model="ir.ui.view">
            <field name="name">account.asset.form</field>
            <field name="model">account.asset</field>
            <field name="priority" eval="40"/>
            <field name="inherit_id" ref="account_asset.view_account_asset_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/group/group/field[@name='acquisition_date']" position="after">  
                    <field name="acc_nineteen"/>
                </xpath>
                <xpath expr="//form/header" position="inside">
                    <!-- object is used if you want to call a method which is written in .py file. -->
                    <button name="gl_nineteen" string="GL - 2019" type="object" class="oe_highlight"/>
                </xpath>
            </field>
      </record>
  </data>
</odoo>